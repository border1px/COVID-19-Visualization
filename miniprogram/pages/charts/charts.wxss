/* pages/charts/charts.wxss */
.section{
  margin-top: 50rpx;
}
.no-publish{
  font-size: 12px;
  padding: 30rpx;
}
.lineCanvas{
  width: 750rpx;
  height: 200px;
  /* margin-left: 50rpx; */
}

/* 表格 */
.table {
  border: 0px solid darkgray;
  width: 700rpx;
  margin-left: auto;
  margin-right: auto;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2.5rem;
  align-items: center;
 }
 .tr.history .td {
   color: #C1C1C1;
 }
 .tag-what{
   width: 30rpx;
   height: 30rpx;
   margin-left: 10rpx;
   vertical-align: middle;
 }
 .first-column{
   text-align: left!important;
 }
 .first-column-inner{
  padding-left: 20rpx;
 }
 .td {
  width:40%;
  justify-content: center;
  text-align: center;
  font-size: 14px;
 }
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .th {
  width: 40%;
  justify-content: center;
  background: skyblue;
  color: #fff;
  display: flex;
  height: 2.8rem;
  align-items: center;
  font-size: 16px;
 }
/* 表格 */

.tip-wrapper{
  margin: 30rpx 0 80rpx 0;
  padding: 0 30rpx;
  text-align: left;
  width: 100%;
}
.stress{
  color: #ed5d3c;
  background-color: #fdf5f5;
}
.tip-label{
  font-weight: bold;
  font-size: 14px;
}
.tip-value{
  font-size: 12px;
  color: #66666C;
}